{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "nether_scorched_potato",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/nether_scorched_potato"
        }
      },
      {
        "when": "end_potato",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/end_potato"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/baked_potato"
    }
  }
}