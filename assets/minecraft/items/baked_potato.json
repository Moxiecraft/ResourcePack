{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "nether_scorched_potato",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/nether_scorched_potato"
        }
      },
      {
        "when": "end_potato",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/end_potato"
        }
      },
      {
        "when": "coriander",
        "model": {
          "type": "range_dispatch",
            "property": "count",
            "scale": 64,
            "entries": [
              {
                "threshold": 0,
                "model": {
                  "type": "model",
                  "model": "moxiecraft:item/coriander"
                }
              },
              {
                "threshold": 8,
                "model": {
                  "type": "model",
                  "model": "moxiecraft:item/coriander_stack"
                }
              }
            ]
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/baked_potato"
    }
  }
}