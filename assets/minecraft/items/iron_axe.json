{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "explosive_hammer",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/explosive_hammer"
        }
      },
      {
        "when": "axeideals",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/void_dark_axe"
        }
      },
      {
        "when": "axetruth",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/void_light_axe"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/iron_axe"
    }
  }
}