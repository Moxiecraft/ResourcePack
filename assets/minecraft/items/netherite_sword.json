{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "special_netherite_sword",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/special_netherite_sword"
        }
      },
      {
        "when": "red_netherite_sword",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/red_netherite_sword"
        }
      },
      {
        "when": "red_wand",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/red_wand"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/netherite_sword"
    }
  }
}